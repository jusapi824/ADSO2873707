Proceso ej12
	Definir nombre como cadena;
	Definir valorhoras Como Entero;
	Definir horas Como Entero;
	Definir pago Como Entero;
	Escribir "digite su nombre";
	leer nombre; 
	Escribir "pago por horas trabajada";
	leer valorhoras; 
	Escribir "numero de horas trabajadas";
	leer horas;
	pago <- valorhoras*horas;
	Escribir "el nombre del trabajador es ", nombre;
	Escribir " el pago del ususario por horas trabajada es: ", pago;
FinProceso
